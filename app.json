{
  "expo": {
    "name": "X Game Master",
    "slug": "x-game-master",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.2.1",
    "newArchEnabled": true,
    "orientation": "portrait",
    "primaryColor": "#222B45",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#222B45"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.iimog.xapp",
      "supportsTablet": true,
      "buildNumber": "11",
      "icon": "./assets/apple_icon.png",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "org.iimog.xapp",
      "versionCode": 11,
      "permissions": [],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=org.iimog.xapp"
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components"
          ]
        }
      }
    },
    "description": "Your companion app to organize a fun game night (or day)",
    "githubUrl": "https://github.com/iimog/x-game-master",
    "extra": {
      "eas": {
        "projectId": "cbb9131c-d0b4-433b-b23a-d1146329e52e"
      }
    }
  }
}
